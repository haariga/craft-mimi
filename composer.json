{
    "name": "haariga/craft-mimi",
    "description": "Generate share images with preview",
    "type": "craft-plugin",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "mimi"
    ],
    "support": {
        "docs": "https://github.com/haariga/craft-mimi/blob/master/README.md",
        "issues": "https://github.com/haariga/craft-mimi/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Haariga",
            "homepage": "https://xn--hriga-mra.com/"
        },
        {
            "name": "Martin Herweg",
            "homepage": "https://github.com/martinherweg"
        },
        {
            "name": "David Hellmann",
            "homepage": "https://davidhellmann.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "haariga\\mimi\\": "src/"
        }
    },
    "extra": {
        "name": "Mimi",
        "handle": "mimi",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/haariga/craft-mimi/master/CHANGELOG.md",
        "components": {
            "mimiService": "haariga\\mimi\\services\\MimiService"
        },
        "class": "haariga\\mimi\\CraftMimi"
    }
}
